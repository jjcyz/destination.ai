[run]
source = app
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */migrations/*
    */test_*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    if settings.debug:
    if settings.log_level:

[html]
directory = htmlcov

